   "source": [
    "#transpose matrix \n",
    "\n",
    "def trans(a):\n",
    "    c = [[None]*len(a) for row in range(len(a[0]))]\n",
    "    for i in range(len(a[0])):\n",
    "        for j in range(len(a)):\n",
    "            c[i][j] = a[j][i]\n",
    "    return c\n",
    "\n",
    "a = [[1,2,3,4],[5,6,7,8]]\n",
    "trans(a)"
   ]
  }
